@model  IEnumerable<CBUSA.Models.CustomQestionControlVM>


<div id="DivCustomQuestionSearch" class="searchSeconDiv">
    <input type="text" id="TxtProjectSearch" />
</div>

<div id="DivCustomQuestionSelect">
    <div style="float:left; margin:0px 10px 0px 30%;">

        <a href="javascript:void(0)" id="ChkSelectAll"><span> Select All</span></a>
    </div>
    <div style="float:left;">

        <a href="javascript:void(0)" id="ChkClearAll"> <span> Clear All</span></a>
    </div>
    <div class="clearfix"></div>
</div>
<div>
    @if (Model != null)
    {
        <table style="width:100%" class="ShowProductTable">
            @foreach (var Itemchild in Model)
            {
                <tr>
                    <td style="width:20%" align="center">
                        <input type="checkbox" name="ChkProjectSelect" />
                        <input type="hidden" value="@Itemchild.QuestionId" name="HdnProjectId" />
                    </td>
                    <td style="width:80%">
                        <span>@Itemchild.QuestionValue</span>
                    </td>
                </tr>
            }
        </table>
    }
</div>