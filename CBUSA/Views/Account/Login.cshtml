<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="cache-control" content="max-age=0" />
    <meta http-equiv="cache-control" content="no-cache" />
    <meta http-equiv="expires" content="0" />
    <meta http-equiv="expires" content="Tue, 01 Jan 1980 1:00:00 GMT" />
    <meta http-equiv="pragma" content="no-cache" />
    <title>CBUSA-NCp</title>

    <!-- Stylesheet -->
    <link href="@Url.Content("~/content/css/style.css")" rel="stylesheet">
    <link href="@Url.Content("~/content/css/bootstrap.min.css")" rel="stylesheet">
    <link href="@Url.Content("~/content/css/font-awesome.min.css")" rel="stylesheet">
    <link rel="shortcut icon" href="@Url.Content("~/Content/images/favicon.ico")" type="image/x-icon" />
    <link rel="shortcut icon" href="@Url.Content("~/Content/images/favicon.png")" type="image/x-icon" />

    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <!-- Start Header -->
    <header class="head-wrapper">
        <div class="container">
            <div class="col-md-4"><a href="#" title="CBUSA"><img src="@Url.Content("~/content/images/logo.png")" class="logo" alt="CBUSA"></a></div>
            <div class="col-md-8"></div>
        </div>
    </header>
    <!-- End Header -->
    <!-- Start Body -->
    <div class="main-body-wrapper">
        <div class="container">

            @if (ViewBag.IsUnauthorizeBuilder)
            {
                <h4 class="text-uppercase maginTop20 text-center colorGray">You are not authorized to access this portal</h4>
            }

            @if (ViewBag.IsArchiveBuilder)
            {
                <h5 class="maginTop20 text-center colorGray">Your account has been de-activated. Kindly contact <a href="mailto:customerservice@cbusa.us">customerservice@cbusa.us</a></h5>
            }

        </div>
    </div>
    <!-- End Body -->
    <!-- Start popup -->
    <!-- Start Manage Status -->
    <!-- End Manage Status -->
    <!-- Start Upload Resources -->
    <!-- End Upload Resources -->
    <!-- End popup -->
    <!-- Start Footer -->
    <footer class="footer-wrapper">
        <div class="container"></div>
    </footer>
    <!-- End Footer -->
    <!-- Jquery Library -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <script src="@Url.Content("~/scripts/bootstrap.min.js")"></script>
</body>
</html>