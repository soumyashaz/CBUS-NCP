@{
    var controller = HttpContext.Current.Request.RequestContext.RouteData.Values["Controller"].ToString();
    string cLayout = "";
    if (controller == "ContractCentral")
    {
        cLayout = "~/Views/Shared/_LayoutContractCentral.cshtml";
    }
    else
    {
        cLayout = "~/Views/Shared/_LayoutBuilder.cshtml";
    }
    Layout = cLayout;
}